{
    "name": "zikula/settings-module",
    "version": "3.1.0-dev",
    "description": "System settings administration",
    "type": "zikula-system-module",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Zikula",
            "homepage": "https://ziku.la/"
        }
    ],
    "autoload": {
        "psr-4": { "Zikula\\SettingsModule\\": "" }
    },
    "require": {
        "php": ">=7.2.5",
        "symfony/config": "^5.2",
        "symfony/contracts": "^5.2",
        "symfony/dependency-injection": "^5.2",
        "symfony/event-dispatcher": "^5.2",
        "symfony/finder": "^5.2",
        "symfony/form": "^5.2",
        "symfony/http-foundation": "^5.2",
        "symfony/http-kernel": "^5.2",
        "symfony/intl": "^5.2",
        "symfony/routing": "^5.2",
        "symfony/security": "^5.2",
        "symfony/string": "^5.2",
        "symfony/validator": "^5.2",
        "php-translation/extractor": "2.*",
        "php-translation/symfony-bundle": "0.*",
        "php-translation/symfony-storage": "2.*",

        "zikula/core-bundle": "self.version",
        "zikula/formextension-bundle": "self.version",
        "zikula/blocks-module": "self.version",
        "zikula/extensions-module": "self.version",
        "zikula/menu-module": "self.version",
        "zikula/permissions-module": "self.version",
        "zikula/theme-module": "self.version",
        "zikula/users-module": "self.version",

        "willdurand/js-translation-bundle": "3.*"
    },
    "extra": {
        "zikula": {
            "class": "Zikula\\SettingsModule\\ZikulaSettingsModule",
            "core-compatibility": ">=3.0.0",
            "displayname": "General settings",
            "url": "settings",
            "icon": "fas fa-tools",
            "capabilities": {
                "admin": {
                    "route": "zikulasettingsmodule_settings_mainsettings"
                }
            },
            "securityschema": {
                "ZikulaSettingsModule::": "::",
                "LocaleBlock::": "::",
                "LocaleBlock::bid": "::bid"
            }
        }
    }
}
